{
  "name": "@cedricziel/faro-injection",
  "version": "0.29.0",
  "description": "A browser extension that injects Faro instrumentation into any website",
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc --build tsconfig.json",
    "build": "npx webpack --mode=production",
    "build:mv2": "npx webpack --mode=production --env MV=2",
    "build:mv3": "npx webpack --mode=production --env MV=3",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precompile": "tsc --version && lerna run version:update --scope @opentelemetry/browser-extension-autoinjection --include-dependencies",
    "prewatch": "npm run precompile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "watch": "npx webpack --mode=development --watch",
    "watch:mv2": "npx webpack --mode=development --watch --env MV=2",
    "watch:mv3": "npx webpack --mode=development --watch --env MV=3"
  },
  "private": true,
  "keywords": [],
  "author": "Cedric Ziel",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.217",
    "@types/jsdom": "16.2.13",
    "@types/mocha": "10.0.1",
    "@types/react": "17.0.16",
    "@types/react-dom": "18.0.2",
    "@types/sinon": "10.0.2",
    "@types/sinon-chrome": "2.2.11",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.8.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-json5": "0.1.4",
    "gts": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jimp": "0.22.5",
    "jsdom": "^21.1.0",
    "mocha": "^10.2.0",
    "null-loader": "4.0.1",
    "nyc": "15.1.0",
    "responsive-loader": "2.3.0",
    "rimraf": "3.0.2",
    "sinon": "15.0.1",
    "sinon-chrome": "3.0.1",
    "ts-loader": "^9.2.5",
    "ts-mocha": "10.0.0",
    "typescript": "^4.4.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.0.0",
    "@grafana/faro-web-tracing": "^1.0.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "change-case": "4.1.2",
    "json5": "2.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "homepage": "https://github.com/cedricziel/grafana-faro-browser-extension"
}
